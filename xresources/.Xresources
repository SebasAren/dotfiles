Xft.dpi:                    75
Xft.antialias:              true
Xft.hinting:                true
Xft.rgba:                   rgb
Xft.autohint:               true
Xft.hintstyle:              hintfull
Xft.lcdfilter:              lcddefault

URxvt*highlightColor:       grey
URxvt*highlightTextColor:   black
URxvt*iso14755_52:          false
URxvt*iso14755:             false
URxvt*visualBell:           true
URxvt*geometry:             238x82
URxvt*scrollBar:            false
URxvt*borderLess:           true
URxvt*externalBorder:       0
URxvt*internalBorder:       5
URxvt*loginShell:           true
URxvt*perl-ext-common:      fullscreen
URxvt*keysym.F11:           perl:fullscreen:switch
URxvt*inheritPixmap:        true
URxvt*transparent:          true
URxvt*shading:              60
URxvt*foreground:           White
URxvt*font:                 xft:Hack Nerd Font:style=Regular:size=10:antialias=true:hinting=true:autohint=true
URxvt*boldFont:             xft:Hack Nerd Font:style=Bold:size=10:antialias=true:hinting=true:autohint=true
URxvt*italicFont:           xft:Hack Nerd Font:style=Italic:size=10:antialias=true:hinting=true:autohint=true
URxvt*scrollTtyOutput:      false
URxvt*fading:               50
URxvt*keysym.Control-Up:    \033[1;5A
URxvt*keysym.Control-Down:  \033[1;5B
URxvt*keysym.Control-Left:  \033[1;5D
URxvt*keysym.Control-Right: \033[1;5C
URxvt*lineSpace:            0
URxvt*letterSpace:          1
