{
  "mcpServers": {
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx",
      "disabled": true
    },
    "filesystem": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${cmd: git rev-parse --show-toplevel}"
      ],
      "command": "npx",
      "disabled": true
    },
    "git": {
      "args": [
        "mcp-server-git",
        "--repository",
        "${cmd: git rev-parse --show-toplevel}"
      ],
      "command": "uvx",
      "disabled": true
    },
    "gitlab": {
      "args": [
        "mcp-server-gitlab"
      ],
      "command": "uvx",
      "disabled": true,
      "env": {
        "GITLAB_API_URL": "${GITLAB_API_URL}",
        "GITLAB_PERSONAL_ACCESS_TOKEN": "${GITLAB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "playwright": {
      "args": [
        "run",
        "-i",
        "--rm",
        "--init",
        "--pull=always",
        "mcr.microsoft.com/playwright/mcp"
      ],
      "command": "docker",
      "disabled": true
    },
    "puppeteer": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "command": "npx",
      "disabled": true
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "custom_instructions": {
        "disabled": false
      },
      "disabled": false,
      "disabled_prompts": [
        "create_native_server"
      ],
      "disabled_resources": [],
      "disabled_tools": []
    },
    "neovim": {
      "autoApprove": [],
      "disabled": true,
      "disabled_prompts": [
        "parrot"
      ],
      "disabled_resources": [],
      "disabled_tools": []
    }
  }
}
