{
  "mcpServers": {
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "autoApprove": [
        "resolve-library-id",
        "get-library-docs"
      ],
      "command": "npx",
      "disabled": false,
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "1000"
      }
    },
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx",
      "disabled": true
    },
    "git": {
      "args": [
        "mcp-server-git",
        "--repository",
        "${cmd: git rev-parse --show-toplevel}"
      ],
      "autoApprove": [
        "git_status",
        "git_diff_unstaged",
        "git_diff_staged",
        "git_diff",
        "git_commit",
        "git_add",
        "git_log",
        "git_show"
      ],
      "command": "uvx",
      "disabled": false,
      "disabled_tools": []
    },
    "gitlab": {
      "args": [
        "mcp-server-gitlab"
      ],
      "command": "uvx",
      "disabled": true,
      "env": {
        "GITLAB_API_URL": "${GITLAB_API_URL}",
        "GITLAB_PERSONAL_ACCESS_TOKEN": "${GITLAB_PERSONAL_ACCESS_TOKEN}"
      }
    },
    "playwright": {
      "args": [
        "run",
        "-i",
        "--rm",
        "--init",
        "--pull=always",
        "mcr.microsoft.com/playwright/mcp"
      ],
      "command": "docker",
      "disabled": true
    },
    "puppeteer": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "command": "npx",
      "disabled": true
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "autoApprove": [
        "sequentialthinking"
      ],
      "command": "npx",
      "disabled": false
    },
    "serena": {
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        "${cmd: pwd}"
      ],
      "command": "uvx",
      "custom_instructions": {
        "disabled": false
      },
      "disabled": true
    },
    "tavily": {
      "args": [
        "-y",
        "tavily-mcp"
      ],
      "autoApprove": [
        "tavily-search"
      ],
      "command": "npx",
      "disabled": false,
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "vectorcode": {
      "command": "vectorcode-mcp-server",
      "disabled": true
    }
  },
  "nativeMCPServers": {
    "conventional_commits": {
      "disabled": false,
      "disabled_tools": [
        "get_status"
      ]
    },
    "mcphub": {
      "custom_instructions": {
        "disabled": false
      },
      "disabled": true,
      "disabled_prompts": [],
      "disabled_resources": [],
      "disabled_tools": []
    },
    "neovim": {
      "autoApprove": [],
      "disabled": false,
      "disabled_prompts": [
        "parrot"
      ],
      "disabled_resources": [],
      "disabled_tools": [
        "delete_items",
        "edit_file",
        "find_files",
        "list_directory",
        "move_item",
        "read_file",
        "read_multiple_files",
        "write_file"
      ]
    },
    "vitest": {
      "disabled": true
    }
  }
}
